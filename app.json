{
  "expo": {
    "name": "Encryptify",
    "slug": "encryptifyapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "your-app-scheme",
    "projectId": "d7e292be-b3e9-4c2e-9ae7-46dec613294a",
    "splash": {
      "image": "./assets/images/encryptifySplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },

    
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.encryptify.app",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access camera.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone",
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "package": "com.encryptify.app",
      "versionCode": 1,
      "googleServicesFile": "./android/app/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#00e701"
      },
      "permissions": [
        "android.permission.CAMERA"
      ],
      "notification": {
        "icon": "./assets/images/notif-icon.png",
        "color": "#00e701"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d7e292be-b3e9-4c2e-9ae7-46dec613294a"
      }
    },
    "owner": "riadjoul"
  }
}
